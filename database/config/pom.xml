    <artifactId>kme-database-config</artifactId>
    <name>Database Configuration</name>
    <dependencies />
    <build>
        <filters>
            <!-- <filter>${user.home}/kme-filters.properties</filter> -->
        </filters>
        <resources>
            <resource>
                <directory>${basedir}/src/main/resources</directory>
